@page "/Asset/{indexKey:long}"
@inherits AssetInspectorBase

@if (Initialized)
{
    @if (DataObject != null)
    {
        <MudGrid>

            <MudItem xs="12">
                <JMayer.Example.WebAssemblyBlazor.Client.Pages.Assets.Cards.AssetOverviewCard @bind-DataObject="@DataObject" />
            </MudItem>

        </MudGrid>
    }
    else
    {
        <NotFound ObjectTypeName="Asset" />
    }
}